{
  "name": "corvo-cortex",
  "version": "2.2.0",
  "type": "module",
  "description": "Serverless AI Gateway with intelligent routing",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "audit": "npm audit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:report": "eslint src --ext .ts,.tsx -f html -o reports/eslint-report.html",
    "complexity": "madge --circular --extensions ts,tsx src/ > reports/complexity.txt",
    "type-check": "tsc --noEmit",
    "analyze": "npm run lint:report && npm run complexity && npm run type-check && npm run test:coverage"
  },
  "keywords": [
    "ai",
    "gateway",
    "cloudflare",
    "workers"
  ],
  "author": "Corvo Labs",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=11.7.0"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "langfuse": "^3.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.1.0",
    "@cloudflare/workers-types": "^4.20231218.0",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-formatter-html": "^2.7.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-security": "^3.0.1",
    "madge": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "wrangler": "^4.54.0"
  }
}
